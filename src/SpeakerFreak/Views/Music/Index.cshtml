@{
    ViewData["Title"] = "Log In";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model IEnumerable<SpeakerFreak.Models.Music>


<div class="container">
    <h2>Top Selling Artists</h2>
    
    @foreach (var listMuscians in Model)
    {
        <h2> @Html.DisplayFor(modelItem => listMuscians.Artist)</h2>
        <img src="@listMuscians.Image_url" alt="Album Cover" />
        <h2>@Html.ActionLink("More Info","Details", new {id = listMuscians.Id })</h2>

    }

</div>


